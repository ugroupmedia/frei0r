<!DOCTYPE kpartgui>
<effect LC_NUMERIC="C" tag="frei0r.cropNwarp" id="frei0r.cropNwarp">
	<name>cropNwarp</name>
	<description>opencv c0rners2 with crop</description>
	<author>RG</author>

	<parameter type="animated" widget="corners" name="0" default="0.333333333333333" min="0" max="6000" factor="6000">
		<name>Corner 1 X</name>
	</parameter>

	<parameter type="animated" widget="corners" name="1" default="0.333333333333333" min="0" max="6000" factor="6000">
		<name>Corner 1 Y</name>
	</parameter>

	<parameter type="animated" widget="corners" name="2" default="0.666666666666667" min="0" max="6000" factor="6000">
		<name>Corner 2 X</name>
	</parameter>

	<parameter type="animated" widget="corners" name="3" default="0.333333333333333" min="0" max="6000" factor="6000">
		<name>Corner 2 Y</name>
	</parameter>

	<parameter type="animated" widget="corners" name="4" default="0.666666666666667" min="0" max="6000" factor="6000">
		<name>Corner 3 X</name>
	</parameter>

	<parameter type="animated" widget="corners" name="5" default="0.666666666666667" min="0" max="6000" factor="6000">
		<name>Corner 3 Y</name>
	</parameter>

	<parameter type="animated" widget="corners" name="6" default="0.333333333333333" min="0" max="6000" factor="6000">
		<name>Corner 4 X</name>
	</parameter>

	<parameter type="animated" widget="corners" name="7" default="0.666666666666667" min="0" max="6000" factor="6000">
		<name>Corner 4 Y</name>
	</parameter>

	<parameter type="list" name="8" default="0.25" paramlist="0;0.25;0.50;0.75;1.0">
		<paramlistdisplay>INTER_NEAREST,INTER_LINEAR,INTER_AREA,INTER_CUBIC,INTER_LANCZOS4</paramlistdisplay>
		<name>Smoothing method</name>
	</parameter>

	<parameter type="animated" name="9" default="0.05" min="0" max="2000" factor="2000">
		<name>Left</name>
	</parameter>

	<parameter type="animated" name="10" default="0.05" min="0" max="2000" factor="2000">
		<name>Top</name>
	</parameter>

	<parameter type="animated" name="11" default="0.95" min="0" max="2000" factor="2000">
		<name>Right</name>
	</parameter>

	<parameter type="animated" name="12" default="0.95" min="0" max="2000" factor="2000">
		<name>Bottom</name>
	</parameter>

	<parameter type="bool" name="13" min="0" max="1" default="0">
		<name>Activate Warp</name>
	</parameter>

	<parameter type="bool" name="14" min="0" max="1" default="1">
		<name>input is a static PNG</name>
	</parameter>

	<parameter type="animated" name="15" default="0"  min="0" max="100" factor="100">
		<name>edge smoothing strength</name>
	</parameter>

	<parameter type="animated" name="16" min="0" max="10" default="0">
		<name>Unique Identifier</name>
	</parameter>
</effect>